let gcd(a, b) begin
    if (a == 0)
    begin
       return b;
    end
    if (b == 0)
    begin
       return a;
    end
    if (a == b)
    begin
        return a;
    end
    if (a > b)
    begin
        return gcd(a-b, b);
    end
    return gcd(a, b-a);
end
  

int a = 98;
int b = 56;
print(gcd(a,b));